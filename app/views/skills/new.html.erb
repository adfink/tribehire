
<div class="container">
  <div class="row">
    <div class="card-panel hoverable teal white-text center-align">Tribe4Hire</div>
    <%= form_for(@skill, multipart: true) do |f| %>
      <div class="field">
        <%= f.label :name %>
        <%= f.text_field :name %>
      </div>
      <div class="actions">
        <%= f.submit 'add this skill' %>
      </div>
    <% end %>

  </div>
</div>
